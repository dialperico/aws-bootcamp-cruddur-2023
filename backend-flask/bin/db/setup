#! /usr/bin/bash
-e #stop process if it fails at any point.

CYAN='\033[1;36m'
NO_COLOR='\033[0m'
LABEL='db-setup'
printf "${CYAN}== ${LABEL}${NO_COLOR}\n"


echo "executing dp-schema-load proccess"
bin_path="$(realpath .)/bin"

source "$bin_path/db/drop"
source "$bin_path/db/create"
source "$bin_path/db/schema-load"
source "$bin_path/db/seed"
source "$bin_path/db/update_congito_user_ids"
